{
  "name": "habitus33-monorepo",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=frontend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build:frontend": "npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=backend",
    "lint": "npm run lint --workspace=frontend && npm run lint --workspace=backend",
    "test": "npm test --workspace=frontend && npm test --workspace=backend",
    "start": "next start",
    "update-zengo-data": "node scripts/update-zengo-data.js",
    "check-zengo-data": "node scripts/check-zengo-data.js",
    "analyze-zengo-distribution": "node scripts/analyze-zengo-distribution.js",
    "insert-zengo-proverbs": "node scripts/insert-zengo-proverbs.js",
    "validate-zengo-proverbs": "node scripts/insert-zengo-proverbs.js --validate-only",
    "check-duplicate-words": "node scripts/validate-duplicate-words.js",
    "test-analytics": "node scripts/test-analytics.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@react-three/drei": "^9.109.2",
    "@react-three/fiber": "^8.16.8",
    "@types/p5": "^1.7.6",
    "@types/three": "^0.167.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "framer-motion": "^12.18.1",
    "lucide-react": "^0.503.0",
    "next": "^14.2.8",
    "p5": "^2.0.3",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^2.10.4",
    "three": "^0.167.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.13",
    "chalk": "^4.1.2",
    "node-fetch": "^2.6.9",
    "typescript": "^5.5.4"
  }
}
