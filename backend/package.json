{
  "name": "habitus33-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fix-zengo-word-counts": "ts-node src/scripts/fixZengoWordCounts.ts",
    "start": "node dist/app.js",
    "dev": "PORT=8000 ts-node --transpile-only src/app.ts",
    "dev:watch": "PORT=8000 nodemon --exec ts-node --transpile-only src/app.ts",
    "build": "tsc",
    "test": "jest",
    "test:integration": "jest --testMatch '**/tests/**/*.integration.test.ts'",
    "test:lint-jsonld": "echo 'JSON-LD linting not implemented yet'",
    "test:shacl": "echo 'SHACL validation not implemented yet'",
    "clean-zengo-data": "ts-node src/scripts/cleanAndUseExpandedData.ts",
    "update-display-times": "ts-node src/scripts/updateDisplayTimes.ts",
    "validate-atlas": "ts-node src/scripts/atlasSchemaValidator.ts",
    "monitor-atlas": "ts-node -e \"import('./src/scripts/atlasSchemaValidator').then(m => m.monitorIndexCreation()).catch(console.error)\"",
    "check-timezone": "ts-node src/scripts/timezoneChecker.ts",
    "validate-timezone": "ts-node -e \"import('./src/scripts/timezoneChecker').then(m => m.diagnoseTimezoneIssues()).catch(console.error)\"",
    "integration-test": "ts-node src/scripts/integrationTest.ts",
    "test-time-system": "ts-node -e \"import('./src/scripts/integrationTest').then(m => m.runIntegrationTests()).catch(console.error)\"",
    "monitor": "ts-node src/scripts/deploymentMonitor.ts single",
    "monitor-live": "ts-node src/scripts/deploymentMonitor.ts continuous",
    "deploy-check": "npm run check-timezone && npm run validate-atlas && npm run integration-test",
    "post-deploy": "npm run monitor",
    "_deprecated_seed-zengo": "echo \"⚠️ 이 스크립트는 더 이상 사용되지 않습니다. clean-zengo-data를 사용하세요.\" && exit 1",
    "_deprecated_expand-zengo": "echo \"⚠️ 이 스크립트는 더 이상 사용되지 않습니다. clean-zengo-data를 사용하세요.\" && exit 1"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.21.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.8.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nanoid": "^3.3.7",
    "stripe": "^14.21.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.22",
    "@types/express-validator": "^2.20.33",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.47",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "jsonld-lint": "^0.3.0",
    "nodemon": "^2.0.22",
    "prisma": "^5.11.0",
    "shacl-engine": "^1.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
